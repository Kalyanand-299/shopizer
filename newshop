pipeline {
    agent {label 'GRADLE && MVN3 && NVM && PY3'} 
    stages {
        stage('clone') { 
            steps {
                git url:'https://github.com/Kalyanand-299/shopizer.git' , branch : 'develop' 
            }
        }
        stage('install') { 

                sh 'mvn package'
            }
        }
    }